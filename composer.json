{
  "name": "zanphp/config",
  "require": {
    "php": ">=5.6",
    "zanphp/contracts": "dev-master",
    "zanphp/support": "dev-master",
    "zanphp/testing":"dev-laogui"
  },
  "autoload": {
    "psr-4": {
      "ZanPHP\\Config\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZanPHP\\Container\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "preferred-install": "dist",
    "secure-http": false
  },
  "minimum-stability": "dev"
}